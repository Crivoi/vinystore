RewriteEngine On
RewriteBase /

RewriteRule ^home$ /php/index.php [NC,L]

RewriteRule ^records/[0-9]+$ /php/record.php [NC,L]

RewriteRule ^users/[0-9]+$ /php/profile.php [NC,L]
RewriteRule ^users/[0-9]+/wishlist /php/wishlist.php [NC,L]
RewriteRule ^users/[0-9]+/my_records$ /php/my_records.php [NC,L]
RewriteRule ^users/[0-9]+/add_record$ /php/add_record.php [NC,L]

RewriteRule ^artists/[0-9]*$ /php/artists_list.php [NC,L]

RewriteRule ^test$ /php/test.php [NC,L]
RewriteRule ^test/.*$ /php/test.php [NC,L]


RewriteRule ^vinyls$ /php/api.vinyls.php [NC,L]
